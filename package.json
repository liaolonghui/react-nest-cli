  "keywords": [
    "react",
    "nest",
    "脚手架"
  ],
  "author": "liaolonghui",
  "license": "ISC",
  "dependencies": {
    "commander": "^14.0.3",
    "consola": "^3.4.2",
    "figlet": "^1.10.0",
    "fs-extra": "^11.3.3",
    "giget": "^3.1.2",
    "ora": "^9.3.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "tsup": "^8.5.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.1",
    "@types/prompts": "^2.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
